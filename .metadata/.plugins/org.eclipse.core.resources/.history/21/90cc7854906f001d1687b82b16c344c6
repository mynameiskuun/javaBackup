package project_1129;

public class googoodan {

	public static void main(String[] args) {
		

		int goo[][] = new int[20][20];
		
		for(int i=2; i<9; i++) {
			for(int j=1; j<2; j++) {
				goo[i][j] = i;
				System.out.print(j + "X " + i + "= " + (j*i) + goo[i][j]);
			}
			System.out.println();
		}
		
		
	}

}
