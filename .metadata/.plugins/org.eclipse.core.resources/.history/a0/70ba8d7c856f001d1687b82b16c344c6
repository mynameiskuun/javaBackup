package project_1129;

public class ArrEx {

	public static void main(String[] args) {

		
		int arr1[] = {2,3,4,5,6};
		System.out.println(arr1.length);
		String[] asd = new String[2];
		asd[0] = "asd";
		asd[1] = "eree";
		System.out.println(asd.length);
		System.out.println(asd[0]);
		System.out.println(asd[1]);
		
	
		
		for(int i=0; i<10; i++) {
			int[] qwe = new int[10];
			qwe[i] = i;
		}
		System.out.println(qwe[8]);
		
	}

}
