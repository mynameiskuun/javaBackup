package project_1129;

public class googoodan {

	public static void main(String[] args) {
		int goo[][] = new int[10][10];
		
		for(int i=2; i<9; i++) {
			for(int j=1; j<=9; j++) {
				goo[i][j] = j*i;
			}
		}
			for(int j=1; j<=9; j++) {
			for(int i=2; i<9; i++)  {
				System.out.print(goo[i][j] + "\t");
			}
			System.out.println();
		}
//		for(int i=1; i<=9; i++) {
//			for(int j=2; j<=9; j++) {
//				System.out.println(j + " * " + i + " = " + (j*i));
//			}
//		}
		
	}

}
