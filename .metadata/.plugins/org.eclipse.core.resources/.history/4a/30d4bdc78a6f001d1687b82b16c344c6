package project_1129;

public class ArrEx {

	public static void main(String[] args) {

		
		int arr1[] = {2,3,4,5,6};
		System.out.println(arr1.length);
		
		String[] asd = new String[2];
		asd[0] = "asd";
		asd[1] = "eree";
		
		System.out.println(asd.length);
		System.out.println(asd[0]);
		System.out.println(asd[1]);
		
	
		int[] qwe = new int[5];
		for(int i=0; i<qwe.length; i++) {
			qwe[i] = i;
		}
		System.out.println(qwe[3]);
		
		System.out.println(sum(arr1));
		
		int[] arr3 = new int[20];
		
		sum1(arr3);
		System.out.println(arr3[17]);
		
		
	}
	
	public static int sum(int a[]) {
		int sum1 = 0;
		
		for(int i=0; i<a.length; i++) {
			sum1 += a[i];
		}
		return sum1;
	}
	
	public static void sum1(int b[]) {
		
		for(int i=0; i<b.length; i++) {
			b[i] = i;
		}
		
	}
	// 배열의 값을 전부 더해서 합계를 return 시키는 메소드 작성

}
