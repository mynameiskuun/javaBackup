package project_1128;

public class StudentEx {

	public static void main(String[] args) {
		
		Students st = new Students();
		st.stuArr = new Student[10];
		st.init();
		System.out.println(st.searchStudent("03"));
		st.updateScore("03",100,100,100);
		System.out.println(st.searchStudent("03"));
		
		st.insertStudent(2,"aa","aa","aa",2,4,1);
		
		Student asd = st.insertStudent(2,"aa","aa","aa",2,4,1);
		
		System.out.println(asd);
		System.out.println(asd.length);
		
	}

}
