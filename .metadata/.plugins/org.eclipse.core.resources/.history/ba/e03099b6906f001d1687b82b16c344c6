package project_1129;

public class googoodan {

	public static void main(String[] args) {
		

		int goo[][] = new int[20][20];
		
		for(int i=2; i<9; i++) {
			for(int j=1; j<9; j++) {
				goo[i][j] = j;
				System.out.print(i + " X " + j + "= " + (j*i) + goo[i][j] + "\t");
			}
			System.out.println();
		}
		
		
	}

}
