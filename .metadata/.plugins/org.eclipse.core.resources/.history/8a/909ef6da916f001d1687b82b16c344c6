package project_1129;

public class googoodan {

	public static void main(String[] args) {
		
		int goo[][] = new int[20][20];
		
		for(int i=2; i<9; i++) {
			for(int j=1; j<=9; j++) {
//				goo[0][i] = j;
				System.out.println(i + " X " + j + "= " + (j*i) + "\t");
			}
			System.out.println();
		}
		
		
	}

}
