package project_1124;

public class Car1 {

	// 필드
	String company = "현대자동차";
	String model;
	String color;
	int maxSpeed;

	// 생성자
	Car1() {
		// 디폴트 생성자.
		// 매개변수가 포함된 생성자
	}

	Car1(String model) {
		this.model = model;
	}

	Car1(String model, String color) {
		this.model = model;
		this.color = color;
	}

	Car1(String model, String color, int maxSpeed) {
		this.model = model;
		this.color = color;
		this.maxSpeed = maxSpeed;
	}

	public static void main(String[] args) {
		Car1 c1 = new Car1("자동차 1");
		System.out.println(c1.company);
		System.out.println(c1.model);
		System.out.println(c1.color);
		System.out.println(c1.maxSpeed);
		
		Car1 c2 = new Car1("자동차 1", "빨간색");
		System.out.println(c2.company);
		System.out.println(c2.model);
		System.out.println(c2.color);
		
		Car1 c3 = new Car1("자동차 1", "빨간색", 200);
		System.out.println(c3.company);
		System.out.println(c3.model);
		System.out.println(c3.color);
		System.out.println(c3.maxSpeed);

	}
}
